{
    "name": "Auto Redirect",
    "description": "Auto Redirect to the desired URL",
    "version": "1.0",
    "manifest_version": 3,
    "background": {
        "service_worker": "background.js"
    },
    "permissions": [
        "declarativeNetRequest"
    ],
    "host_permissions": [
        "*://linkedin.com/*",
        "*://www.linkedin.com/*"
    ]
}